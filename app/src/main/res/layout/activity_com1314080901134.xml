<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.hzuapps.androidworks.homeworks.com1314080901134.com1314080901134">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="网易云音乐界面 1314080901134"
        android:id="@+id/textView" />
    <Button
        android:id="@+id/Button01"
        android:onClick="onClick_Event"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:text="个性推荐"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/Button02"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:text="歌单"
        android:onClick="onClick_Event"
        android:layout_alignTop="@+id/Button01"
        android:layout_toEndOf="@+id/Button01" />
    <Button
        android:id="@+id/Button03"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="主播电台"
        android:onClick="onClick_Event"
        android:layout_alignTop="@+id/Button02"
        android:layout_toEndOf="@+id/Button02" />
    <ImageView
        android:id="@+id/Image1"
        android:src="@drawable/com1314080901134image"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
